<a
  {{#if obj.PAGE_KEY}}
    class="PageLink"
    href="/{{obj.PAGE_KEY}}"
  {{else if obj.EXTERNAL_LINK}}
    class="ExternalLink"
    href="{{obj.EXTERNAL_LINK}}"
  {{else}}
    class="NoLink"
  {{/if}}
    target="_blank">
  {{#if obj.PAGE_KEY}}
    <div class="PageKey">[{{obj.PAGE_KEY}}]</div>
  {{/if}}
  {{obj.D_1}}{{obj.D_2}}{{obj.D_3}}{{obj.D_4}}
</a>