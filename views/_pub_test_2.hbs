<div id='Page'>
  {{> button/Btn_1}}
  {{> layout/Header_1-a}}
  <main>
    {{JSON 'SampleData' '/assets/json/SampleData.json'}}
    <div>name : {{SampleData.name}}</div>
    {{#LOOP 10}}
      <div>
        <div>index : {{index}}</div>
        <div>number : {{number}}</div>
        <div>digit : {{digit}}</div>
      </div>
    {{/LOOP}}
    <div>{{BAR}}</div>
    {{#EACH (ARR '🍎|🍍|🥝|🍇|🍈')}}
      <div>
        {{obj}}
      </div>
    {{/EACH}}
    <img src='https://gifpng.com/560x315/ffd4d4/ff4444' alt='' />
  </main>
  {{> common/footer}}
</div>