    </section>
    <section>
      <h3 class='H3'>Disabling a date range</h3>
      <input id='inp-a1dda77c' type='text' />
      {{!--prettier-ignore--}}
      <script>
        (() => {
          const el_inp = document.querySelector(`#inp-a1dda77c`);
          flatpickr(el_inp, {
            disable: [
              {
                from: "2023-10-01",
                to: "2023-10-03"
              },
              {
                from: "2023-10-11",
                to: "2023-10-14"
              }
            ]
          });
        })()
      </script>
    </section>
    <section>
      <h3 class='H3'>Disabling dates by a function</h3>
      <input id='inp-89400eb6' type='text' />
      {{!--prettier-ignore--}}
      <script>
        (() => {
          const el_inp = document.querySelector(`#inp-89400eb6`);
          flatpickr(el_inp, {
            "disable": [
              function (date) {
                // return true to disable
                return (date.getDay() === 0 || date.getDay() === 6);

              }
            ]
          });
        })()
      </script>
    </section>
    <section>
      <h2 class='H2'>Disabling all dates except select few</h2>
