    <section>
      <h3 class='H3'>Enabling dates by a function</h3>
      <input id='inp-6b053fdc' type='text' />
      {{!--prettier-ignore--}}
      <script>
        (() => {
          const el_inp = document.querySelector(`#inp-6b053fdc`);
          flatpickr(el_inp, {
            enable: [
              function (date) {
                // return true to enable
                return (date.getMonth() % 2 === 0 && date.getDate() < 15);
              }
            ]
          });
        })()
      </script>
    </section>
