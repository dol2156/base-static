<div id='Page'>
  <main class="pb-[20px]">
    <link href='/assets/css/sitemap.css' rel='stylesheet'/>
    <div class='H1 Sticky Top px-[20px]'>SiteMap</div>
    <div id='Sitemap'>
      <ul>
        {{#EACH MENU_DATA}}
          <li>
            {{> etc/SiteMapLink}}
            {{#if obj.CHILD}}
              <ul>
                {{#EACH obj.CHILD}}
                  <li>
                    {{> etc/SiteMapLink}}
                    {{#if obj.CHILD}}
                      <ul>
                        {{#EACH obj.CHILD}}
                          <li>
                            {{> etc/SiteMapLink}}
                            {{#if obj.CHILD}}
                              <ul>
                                {{#EACH obj.CHILD}}
                                  <li>
                                    {{> etc/SiteMapLink}}
                                  </li>
                                {{/EACH}}
                              </ul>
                            {{/if}}
                          </li>
                        {{/EACH}}
                      </ul>
                    {{/if}}
                  </li>
                {{/EACH}}
              </ul>
            {{/if}}
          </li>
        {{/EACH}}
      </ul>
    </div>
  </main>
</div>